{
  "name": "auto-ui",
  "version": "1.0.0",
  "description": "UI component library for the Auto platform.",
  "main": "lib/auto-ui.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.standalone.js",
    "watch": "npm run build -- --watch",
    "flow": "flow check src",
    "lint": "standard \"src/**/*.js\" | snazzy",
    "lint-fix": "standard \"src/**/*.js\" --fix | snazzy",
    "test": "npm run lint && npm run flow",
    "start": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "keywords": [],
  "author": "joshwnj",
  "license": "MIT",
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype",
      "react"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-cmz-names": "^1.5.0",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.2.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "flow-bin": "^0.61.0",
    "jest": "^21.2.1",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-dom-factories": "^1.0.2",
    "react-select": "1.2.1",
    "react-styleguidist": "^6.5.3",
    "snapguidist": "^3.1.2",
    "snazzy": "^8.0.0",
    "source-map-loader": "^0.2.4",
    "standard": "^12.0.1",
    "url-loader": "^1.1.2",
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "cmz": "^3.12.1",
    "color": "^3.1.0",
    "crypto-js": "^3.1.9-1",
    "css-loader": "^1.0.1",
    "date-fns": "^1.29.0",
    "lodash.isequal": "^4.5.0",
    "markdown-to-jsx": "^6.8.3",
    "medium-editor": "^5.23.3",
    "react-click-outside": "github:tj/react-click-outside",
    "recompose": "^0.30.0",
    "uuid": "^3.3.2",
    "webfontloader": "^1.6.28"
  }
